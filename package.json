{
    "name": "f1-garage",
    "version": "0.2.0",
    "description": "Npm package wrapping the jolpica f1 api",
    "main": "./dist/index.js",
    "exports": {
        "./jolpica": {
            "types": "./dist/jolpica/index.d.ts",
            "import": "./dist/jolpica/index.js",
            "require": "./dist/jolpica/index.js"
        },
        "./openf1": {
            "types": "./dist/openf1/index.d.ts",
            "import": "./dist/openf1/index.js",
            "require": "./dist/openf1/index.js"
        }
    },
    "scripts": {
        "_test": "ts-node src/test",
        "build": "tsup",
        "lint": "eslint . --fix --cache",
        "format": "dprint fmt",
        "type-check": "tsc --noEmit",
        "spell-check": "cspell",
        "quality": "concurrently -n ESLint,dprint,TypeScript,CSpell -c #3178C6,#4D5564,#4B32C3,#B51A00 \"npm run lint\" \"npm run format\" \"npm run type-check\" \"npm run spell-check -- --silent\" "
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JoostWG/f1-garage.git"
    },
    "keywords": [
        "f1",
        "api",
        "jolpica",
        "ergast"
    ],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/JoostWG/f1-garage/issues"
    },
    "homepage": "https://github.com/JoostWG/f1-garage#readme",
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@stylistic/eslint-plugin": "^5.7.0",
        "@typescript-eslint/eslint-plugin": "^8.53.0",
        "@typescript-eslint/parser": "^8.53.0",
        "concurrently": "^9.2.1",
        "cspell": "^9.6.0",
        "dprint": "^0.51.1",
        "eslint": "^9.39.2",
        "ts-node": "^10.9.2",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.53.0"
    },
    "dependencies": {
        "axios": "^1.13.2"
    }
}
